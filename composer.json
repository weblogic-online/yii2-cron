    {
      "name": "Dirk Rossmann GmbH",
      "email": "Entwicklung_CRM@rossmann.de",
      "homepage": "https://www.rossmann.de",
      "role": "Developer"
    },
    {
      "name": "Waut Van den Abbeele",
      "email": "waut@weblogiconline.eu",
      "homepage": "https://www.weblogiconline.eu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "yiisoft/yii2-bootstrap4": "^2.0",
    "dragonmantank/cron-expression": "^3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "weblogic\\cron\\": "./src",
      "weblogic\\cron_tests\\": "./tests"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "weblogic\\cron\\": "./src",
      "weblogic\\cron_tests\\": "./tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
