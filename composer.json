{
  "name": "weblogic-online/yii2-cron",
  "description": "Flexible cron tasks manager for the Yii2 Framework",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "yii2-extension",
  "keywords": [
    "cron",
    "crontab",
    "tasks manager",
    "schedule",
    "Yii2",
    "Yii 2"
  ],
  "homepage": "https://github.com/weblogic-online/yii2-cron",
